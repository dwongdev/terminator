{
  "enabled": true,
  "defaultTimeout": 10000,
  "maxConcurrentConnections": 5,
  "retryAttempts": 3,
  "servers": [
    {
      "name": "filesystem",
      "description": "File system operations MCP server",
      "enabled": false,
      "priority": 80,
      "timeout": 15000,
      "transport": {
        "type": "stdio",
        "command": "npx",
        "args": ["-y", "@modelcontextprotocol/server-filesystem@latest", "/tmp"],
        "env": {
          "NODE_ENV": "production"
        }
      }
    },
    {
      "name": "github",
      "description": "GitHub integration MCP server",
      "enabled": false,
      "priority": 70,
      "timeout": 20000,
      "transport": {
        "type": "stdio",
        "command": "npx",
        "args": [
          "-y",
          "@smithery/cli@latest",
          "run",
          "@smithery-ai/github",
          "--config",
          "{\"githubPersonalAccessToken\":\"YOUR_GITHUB_TOKEN_HERE\"}"
        ]
      }
    },
    {
      "name": "kubernetes",
      "description": "Kubernetes cluster management MCP server",
      "enabled": false,
      "priority": 60,
      "transport": {
        "type": "stdio",
        "command": "npx",
        "args": ["-y", "kubernetes-mcp-server@latest"]
      }
    },
    {
      "name": "sqlite",
      "description": "SQLite database operations MCP server",
      "enabled": false,
      "priority": 50,
      "transport": {
        "type": "stdio",
        "command": "npx",
        "args": ["-y", "@modelcontextprotocol/server-sqlite@latest", "database.db"]
      }
    },
    {
      "name": "postgres",
      "description": "PostgreSQL database operations MCP server",
      "enabled": false,
      "priority": 50,
      "transport": {
        "type": "stdio",
        "command": "npx",
        "args": ["-y", "@modelcontextprotocol/server-postgres@latest"],
        "env": {
          "POSTGRES_CONNECTION_STRING": "postgresql://user:password@localhost:5432/database"
        }
      }
    },
    {
      "name": "brave-search",
      "description": "Brave search API MCP server",
      "enabled": false,
      "priority": 40,
      "transport": {
        "type": "stdio",
        "command": "npx",
        "args": ["-y", "@modelcontextprotocol/server-brave-search@latest"],
        "env": {
          "BRAVE_API_KEY": "YOUR_BRAVE_API_KEY_HERE"
        }
      }
    },
    {
      "name": "slack",
      "description": "Slack integration MCP server",
      "enabled": false,
      "priority": 30,
      "transport": {
        "type": "stdio",
        "command": "npx",
        "args": ["-y", "@modelcontextprotocol/server-slack@latest"],
        "env": {
          "SLACK_BOT_TOKEN": "YOUR_SLACK_BOT_TOKEN_HERE"
        }
      }
    },
    {
      "name": "memory",
      "description": "Memory and context management MCP server",
      "enabled": true,
      "priority": 90,
      "transport": {
        "type": "stdio",
        "command": "npx",
        "args": ["-y", "@modelcontextprotocol/server-memory@latest"]
      }
    }
  ]
}